        { "key": "postId", "value": "1", "enabled": true },
        { "key": "recipeId", "value": "1", "enabled": true },
        { "key": "limit", "value": "10", "enabled": true },
        { "key": "skip", "value": "0", "enabled": true },
        { "key": "username", "value": "emilys", "enabled": true },
        { "key": "password", "value": "", "enabled": true, "secret": true },
        { "key": "accessToken", "value": "", "enabled": true, "secret": true }
      ]
    }
  ],
  "activeEnvironmentId": "env-dummyjson",
  "items": [
    {
      "id": "folder-auth",
      "type": "folder",
      "name": "Authentication",
      "items": [
        {
          "id": "req-login",
          "type": "request",
          "name": "Login",
          "method": "POST",
          "url": "{{baseUrl}}/auth/login",
          "params": [],
          "headers": [
            { "key": "Content-Type", "value": "application/json", "enabled": true }
          ],
          "body": {
            "type": "json",
            "content": "{\n  \"username\": \"{{username}}\",\n  \"password\": \"{{password}}\",\n  \"expiresInMins\": 30\n}"
          },
          "scripts": {
            "pre": "console.log('Logging in as:', nk.getVar('username'));",
            "post": "nk.test('Login successful', () => {\n  nk.assert(nk.response.statusCode === 200, 'Expected 200');\n});\n\nnk.test('Has access token', () => {\n  const body = JSON.parse(nk.response.body);\n  nk.assert(body.accessToken, 'Missing accessToken');\n  nk.setVar('accessToken', body.accessToken);\n});"
          },
          "docs": "Authenticates user and returns JWT tokens. Sets `accessToken` variable for subsequent requests."
        },
        {
          "id": "req-current-user",
          "type": "request",
          "name": "Get Current User",
          "method": "GET",
          "url": "{{baseUrl}}/auth/me",
          "params": [],
          "headers": [
            { "key": "Authorization", "value": "Bearer {{accessToken}}", "enabled": true }
          ],
          "body": { "type": "none" },
          "scripts": {
            "pre": "",
            "post": "nk.test('Authenticated request works', () => {\n  nk.assert(nk.response.statusCode === 200, 'Expected 200');\n  const user = JSON.parse(nk.response.body);\n  nk.assert(user.id, 'Missing user id');\n  console.log('Logged in as:', user.firstName, user.lastName);\n});"
          },
          "docs": "Returns the currently authenticated user. Requires valid `accessToken`."
        },
        {
          "id": "req-refresh-token",
          "type": "request",
          "name": "Refresh Token",
          "method": "POST",
          "url": "{{baseUrl}}/auth/refresh",
          "params": [],
          "headers": [
            { "key": "Content-Type", "value": "application/json", "enabled": true }
          ],
          "body": {
            "type": "json",
            "content": "{\n  \"refreshToken\": \"{{refreshToken}}\",\n  \"expiresInMins\": 30\n}"
          },
          "scripts": { "pre": "", "post": "" }
        }
      ]
    },
