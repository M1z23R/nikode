        { "key": "skip", "value": "0", "enabled": true }
      ]
    },
    {
      "id": "env-authenticated",
      "name": "Authenticated",
      "variables": [
        { "key": "baseUrl", "value": "https://dummyjson.com", "enabled": true },
        { "key": "userId", "value": "1", "enabled": true },
        { "key": "productId", "value": "1", "enabled": true },
